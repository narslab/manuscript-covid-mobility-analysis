@article{mesle2019use,
  title = {The Use and Reporting of Airline Passenger Data for Infectious Disease Modelling: A Systematic Review},
  shorttitle = {The Use and Reporting of Airline Passenger Data for Infectious Disease Modelling},
  author = {Mesl{\'e}, Margaux Marie Isabelle and Hall, Ian Melvyn and Christley, Robert Matthew and Leach, Steve and Read, Jonathan Michael},
